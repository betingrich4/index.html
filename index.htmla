                type: 'line',
                data: {
                    labels: ['W1', 'W2', 'W3', 'W4'],
                    datasets: [{
                        data: getConversionData(),
                        borderColor: '#10b981',
                        backgroundColor: '#10b98120',
                        fill: true,
                        tension: 0.4
                    }]
                },
                options: { responsive: true, maintainAspectRatio: false, plugins: { legend: { display: false } } }
            });
            
            // Distribution chart
            const ctx4 = document.getElementById('distributionChart').getContext('2d');
            if (charts.distribution) charts.distribution.destroy();
            charts.distribution = new Chart(ctx4, {
                type: 'doughnut',
                data: {
                    labels: ['Onboards', 'Trades'],
                    datasets: [{
                        data: [
                            reports.filter(r => r.type === 'onboarding').length,
                            reports.filter(r => r.status === 'traded').length
                        ],
                        backgroundColor: ['#013f4a', '#26658c'],
                        borderWidth: 0
                    }]
                },
                options: { responsive: true, maintainAspectRatio: false, plugins: { legend: { position: 'bottom' } }, cutout: '70%' }
            });
        }

        function destroyCharts() {
            Object.values(charts).forEach(chart => {
                if (chart) chart.destroy();
            });
            charts = {};
        }

        function getWeeklyData() {
            const days = [0,0,0,0,0,0,0];
            reports.forEach(r => {
                if (r.status === 'traded') {
                    const d = new Date(r.date);
                    const day = d.getDay(); // 0 = Sunday, 1 = Monday
                    if (day >= 1 && day <= 7) {
                        days[day-1] += 1;
                    }
                }
            });
            return days;
        }

        function getMonthlyData() {
            const weeks = [0,0,0,0];
            reports.forEach(r => {
                if (r.status === 'traded') {
                    const d = new Date(r.date);
                    if (d.getMonth() === 1) { // February
                        const week = Math.floor((d.getDate() - 1) / 7);
                        if (week < 4) weeks[week]++;
                    }
                }
            });
            return weeks;
        }

        function getConversionData() {
            const weeks = [0,0,0,0];
            for (let week = 0; week < 4; week++) {
                const weekReports = reports.filter(r => {
                    const d = new Date(r.date);
                    const reportWeek = Math.floor((d.getDate() - 1) / 7);
                    return d.getMonth() === 1 && reportWeek === week;
                });
                const traded = weekReports.filter(r => r.status === 'traded').length;
                weeks[week] = weekReports.length ? Math.round((traded / weekReports.length) * 100) : 0;
            }
            return weeks;
        }

        // ==================== UTILITIES ====================
        function switchTab(tab) {
            document.querySelectorAll('.nav-item').forEach(item => item.classList.remove('active'));
            event.currentTarget.classList.add('active');
            
            document.querySelectorAll('.tab-content').forEach(content => content.classList.remove('active'));
            document.getElementById(tab + '-tab').classList.add('active');
            
            // Show goals only on home tab
            document.getElementById('goalsContainer').style.display = tab === 'home' ? 'block' : 'none';
        }

        function openAIModal() {
            document.getElementById('aiDuplicateWarning').style.display = 'none';
            document.getElementById('aiModal').classList.add('active');
        }

        function closeAIModal() {
            document.getElementById('aiModal').classList.remove('active');
        }

        function closeSheet() {
            document.getElementById('dateSheet').classList.remove('open');
        }

        function updateDateDisplay() {
            const today = new Date();
            document.getElementById('currentDate').textContent = 
                today.toLocaleDateString('en-GB', { day: 'numeric', month: 'short', year: 'numeric' });
        }

        function showToast(message) {
            const toast = document.createElement('div');
            toast.className = 'toast';
            toast.textContent = message;
            document.body.appendChild(toast);
            setTimeout(() => toast.remove(), 3000);
        }

        function refreshData() {
            renderHome();
            showToast('Data refreshed');
        }

        function toggleTheme() {
            currentTheme = currentTheme === 'light' ? 'dark' : 'light';
            document.documentElement.setAttribute('data-theme', currentTheme);
            localStorage.setItem('theme', currentTheme);
            
            const icon = document.querySelector('.theme-toggle i');
            icon.className = currentTheme === 'light' ? 'fas fa-moon' : 'fas fa-sun';
            
            destroyCharts();
            initCharts();
        }

        function checkTheme() {
            const saved = localStorage.getItem('theme');
            if (saved) {
                currentTheme = saved;
                document.documentElement.setAttribute('data-theme', saved);
                const icon = document.querySelector('.theme-toggle i');
                icon.className = saved === 'light' ? 'fas fa-moon' : 'fas fa-sun';
            }
        }
    </script>
</body>
</html>
