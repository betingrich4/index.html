        async function syncToSupabase(data) {
            try {
                const { error } = await supabaseClient
                    .from('reports')
                    .upsert(data, { onConflict: 'id' });
                if (error) console.error('Sync error:', error);
            } catch (error) {
                console.error('Sync error:', error);
            }
        }

        // ==================== SEARCH FUNCTIONALITY ====================
        function handleSearch() {
            const query = document.getElementById('searchInput').value.toLowerCase().trim();
            const resultsContainer = document.getElementById('searchResults');
            
            if (searchTimeout) clearTimeout(searchTimeout);
            
            searchTimeout = setTimeout(() => {
                if (query.length < 2) {
                    resultsContainer.classList.remove('active');
                    return;
                }
                
                const results = reports.filter(r => 
                    r.shopName.toLowerCase().includes(query) || 
                    r.phoneNumber.includes(query)
                ).slice(0, 10); // Limit to 10 results
                
                if (results.length === 0) {
                    resultsContainer.innerHTML = '<div class="search-result-item">No shops found</div>';
                    resultsContainer.classList.add('active');
                    return;
                }
                
                let html = '';
                results.forEach(r => {
                    const statusClass = r.status === 'traded' ? 'search-result-traded' : 'search-result-pending';
                    const statusText = r.status === 'traded' ? 'Traded' : 'Pending';
                    const typeText = r.type === 'onboarding' ? 'Onboarded' : 'Verified';
                    
                    html += `
                        <div class="search-result-item" onclick="highlightShop('${r.phoneNumber}')">
                            <div>
                                <div class="search-result-name">${r.shopName}</div>
                                <div class="search-result-phone">${r.phoneNumber} | ${r.date}</div>
                            </div>
                            <div>
                                <span class="search-result-badge ${statusClass}">${statusText}</span>
                                <span style="margin-left:6px; font-size:10px; color:var(--text-tertiary);">${typeText}</span>
                            </div>
                        </div>
                    `;
                });
                
                resultsContainer.innerHTML = html;
                resultsContainer.classList.add('active');
            }, 300);
        }

        function highlightShop(phoneNumber) {
            // Close search results
            document.getElementById('searchResults').classList.remove('active');
            document.getElementById('searchInput').value = '';
            
            // Remove any existing highlights
            document.querySelectorAll('.search-highlight-row').forEach(el => {
                el.classList.remove('search-highlight-row');
            });
            
            // Find and highlight all rows with this phone number across all tables
            const allTables = ['todayActivityBody', 'sheetTable', 'allDataTable'];
            allTables.forEach(tableId => {
                const table = document.getElementById(tableId);
                if (table) {
                    const rows = table.getElementsByTagName('tr');
                    for (let row of rows) {
                        if (row.innerHTML.includes(phoneNumber)) {
                            row.classList.add('search-highlight-row');
                            // Scroll to first occurrence
                            if (tableId === 'todayActivityBody') {
                                row.scrollIntoView({ behavior: 'smooth', block: 'center' });
                            }
                        }
                    }
                }
            });
            
            showToast(`Found shops with phone ${phoneNumber}`);
        }

        // ==================== DATE VALIDATION ====================
        function canEditDate(dateStr) {
            const today = new Date().toISOString().split('T')[0];
            const date = new Date(dateStr);
            const trainingEnd = new Date(TRAINING_END_DATE);
            
            // Cannot edit training period (before 14 Feb)
            if (date < trainingEnd) {
                return { canEdit: false, reason: 'training' };
            }
            
            // Cannot edit future dates
            if (dateStr > today) {
                return { canEdit: false, reason: 'future' };
            }
            
            // Can only edit today
            if (dateStr === today) {
                return { canEdit: true, reason: 'today' };
            }
            
            // Past dates with data - view only
            return { canEdit: false, reason: 'past' };
        }

        // ==================== DUPLICATE CHECK ====================
        function checkDuplicateOnboard(phoneNumber) {
            // Check if this phone number already exists as onboarding anywhere in the system
            const existingOnboard = reports.find(r => 
                r.phoneNumber === phoneNumber && 
                r.type === 'onboarding'
            );
            
            if (existingOnboard) {
                return {
                    isDuplicate: true,
                    existingEntry: existingOnboard
                };
            }
            return { isDuplicate: false };
        }

