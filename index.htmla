        function askAssistant() {
            const question = document.getElementById('assistantQuestion').value.toLowerCase();
            let response = '';
            
            // Calculate insights
            const dayPerformance = {};
            reports.forEach(r => {
                if (r.status === 'traded') {
                    dayPerformance[r.date] = (dayPerformance[r.date] || 0) + 1;
                }
            });
            
            // Find best day
            let bestDay = { date: '', count: 0 };
            // Find worst day (among days with data)
            let worstDay = { date: '', count: Infinity };
            
            Object.entries(dayPerformance).forEach(([date, count]) => {
                if (count > bestDay.count) {
                    bestDay = { date, count };
                }
                if (count < worstDay.count && count > 0) {
                    worstDay = { date, count };
                }
            });
            
            // Calculate weekly averages
            const weekDays = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'];
            const dayAverages = {};
            reports.forEach(r => {
                if (r.status === 'traded') {
                    const d = new Date(r.date);
                    const dayName = weekDays[d.getDay() === 0 ? 6 : d.getDay() - 1];
                    dayAverages[dayName] = (dayAverages[dayName] || 0) + 1;
                }
            });
            
            // Find strength day (highest average)
            let strengthDay = { day: '', count: 0 };
            // Find weakness day (lowest average)
            let weaknessDay = { day: '', count: Infinity };
            
            Object.entries(dayAverages).forEach(([day, count]) => {
                if (count > strengthDay.count) {
                    strengthDay = { day, count };
                }
                if (count < weaknessDay.count) {
                    weaknessDay = { day, count };
                }
            });
            
            if (question.includes('best day') || question.includes('perform well')) {
                if (bestDay.date) {
                    const date = new Date(bestDay.date);
                    const dayName = weekDays[date.getDay() === 0 ? 6 : date.getDay() - 1];
                    response = `Your best performance was on ${date.toLocaleDateString('en-GB', { day: 'numeric', month: 'long' })} (${dayName}) with ${bestDay.count} trades.`;
                } else {
                    response = 'No performance data available yet.';
                }
            }
            else if (question.includes('weakness') || question.includes('worst')) {
                if (worstDay.date && worstDay.count !== Infinity) {
                    const date = new Date(worstDay.date);
                    const dayName = weekDays[date.getDay() === 0 ? 6 : date.getDay() - 1];
                    response = `Your weakest day was on ${date.toLocaleDateString('en-GB', { day: 'numeric', month: 'long' })} (${dayName}) with ${worstDay.count} trades. Consider analyzing what went wrong that day.`;
                } else {
                    response = 'No performance data available yet.';
                }
            }
            else if (question.includes('strength') || question.includes('strong')) {
                if (strengthDay.day) {
                    response = `Your strongest day is ${strengthDay.day} with an average of ${(strengthDay.count/4).toFixed(1)} trades. You perform best on ${strengthDay.day}s!`;
                } else {
                    response = 'No performance data available yet.';
                }
            }
            else if (question.includes('weekly') || question.includes('summary')) {
                const weeks = getWeeklySummary();
                response = 'Weekly Performance Summary:\n';
                weeks.forEach((week, index) => {
                    response += `\nWeek ${index + 1}: ${week.trades} trades, ${week.onboards} onboards, ${week.conversion}% conversion`;
                });
            }
            else if (question.includes('total') || question.includes('overall')) {
                const totalTrades = reports.filter(r => r.status === 'traded').length;
                const totalOnboards = reports.filter(r => r.type === 'onboarding').length;
                const conversion = reports.length ? Math.round((totalTrades / reports.length) * 100) : 0;
                response = `Overall Performance:\nTotal Trades: ${totalTrades}\nTotal Onboards: ${totalOnboards}\nOverall Conversion: ${conversion}%\nTotal Shops Visited: ${reports.length}`;
            }
            else {
                response = 'I can help you with:\n- "Which day did I perform best?"\n- "What is my weakness?"\n- "Show my strength day"\n- "Weekly summary"\n- "Overall performance"';
            }
            
            document.getElementById('assistantResponse').innerHTML = response.replace(/\n/g, '<br>');
        }

        // ==================== AI DATA PROCESSING ====================
        function processAIData() {
            const rawText = document.getElementById('aiInput').value;
            const lines = rawText.split('\n');
            
            let currentDate = null;
            let currentSection = null;
            let processed = [];
            let duplicates = [];
            let today = new Date().toISOString().split('T')[0];
            
            for (let line of lines) {
                line = line.trim();
                if (!line) continue;
                
                // Check for date
                if (line.match(/^\d{1,2}\s+[A-Za-z]{3}/)) {
                    const [day, month] = line.split(' ');
                    const months = {'Jan':'01', 'Feb':'02', 'Mar':'03', 'Apr':'04', 'May':'05', 'Jun':'06',
                                   'Jul':'07', 'Aug':'08', 'Sep':'09', 'Oct':'10', 'Nov':'11', 'Dec':'12'};
                    if (months[month]) {
                        currentDate = `2026-${months[month]}-${day.padStart(2,'0')}`;
                        
                        // Validate if we can edit this date
                        const editCheck = canEditDate(currentDate);
                        if (!editCheck.canEdit) {
                            if (editCheck.reason === 'training') {
                                showToast('Cannot add data for training period (before 14 Feb)');
                            } else if (editCheck.reason === 'future') {
                                showToast('Cannot add data for future dates');
                            } else if (editCheck.reason === 'past') {
                                showToast('Past dates cannot be edited - only viewed');
                            }
                            return;
                        }
                    }
                }
                
                // Check for sections
                else if (line.toLowerCase().includes('onboarding') || line.toLowerCase().includes('onboard')) {
                    currentSection = 'onboarding';
                }
                else if (line.toLowerCase().includes('new trade') || line.toLowerCase().includes('trade')) {
                    currentSection = 'newtrade';
                }
                
                // Extract shop data
                else if (line.match(/^\d+\./)) {
                    let text = line.replace(/^\d+\.\s*/, '');
                    const phoneMatch = text.match(/0\d{9}|0\d{2}\s?\d{3}\s?\d{3}|01\d{8}/);
                    
                    if (phoneMatch) {
                        let phoneNumber = phoneMatch[0].replace(/\s/g, '');
                        let shopName = text.replace(phoneMatch[0], '').trim().replace(/[.,]$/, '');
                        
                        if (shopName && phoneNumber && currentDate) {
                            // For onboarding entries - check for duplicates
                            if (currentSection === 'onboarding') {
                                const duplicateCheck = checkDuplicateOnboard(phoneNumber);
                                if (duplicateCheck.isDuplicate) {
                                    duplicates.push({
                                        shopName,
                                        phoneNumber,
                                        existing: duplicateCheck.existingEntry
                                    });
                                    continue; // Skip adding this entry
                                }
                            }
                            
                            // For new trade entries - check if this phone was onboarded before
                            if (currentSection === 'newtrade') {
                                const existingOnboard = reports.find(r => 
                                    r.phoneNumber === phoneNumber && 
                                    r.type === 'onboarding'
                                );
                                
                                if (existingOnboard) {
                                    // Add checkmark to the previous onboarding entry
                                    existingOnboard.checkmark = true;
                                    
                                    // Update in storage
                                    localStorage.setItem('seviReports', JSON.stringify(reports));
                                    syncToSupabase([existingOnboard]);
                                }
                            }
                            
                            processed.push({
                                id: Date.now() + Math.random(),
                                date: currentDate,
                                shopName: shopName,
                                phoneNumber: phoneNumber,
                                type: currentSection || 'onboarding',
                                status: currentSection === 'newtrade' ? 'traded' : 'not-traded',
                                checkmark: false
                            });
                        }
                    }
                }
            }
            
            // Show duplicate warnings
            if (duplicates.length > 0) {
                let message = 'Duplicate onboardings skipped:\n';
                duplicates.forEach(d => {
                    message += `\n• ${d.shopName} (${d.phoneNumber}) already onboarded on ${d.existing.date}`;
                });
                document.getElementById('aiDuplicateMessage').textContent = message;
                document.getElementById('aiDuplicateWarning').style.display = 'flex';
            } else {
                document.getElementById('aiDuplicateWarning').style.display = 'none';
            }
            
            if (processed.length > 0) {
                reports = [...reports, ...processed];
                localStorage.setItem('seviReports', JSON.stringify(reports));
                syncToSupabase(processed);
                
                renderHome();
                renderCalendar();
                updateReports();
                updateProgress();
                updateAnalytics();
                destroyCharts();
                initCharts();
                
                closeAIModal();
                showToast(`Added ${processed.length} shops`);
                document.getElementById('aiInput').value = '';
            } else if (duplicates.length === 0) {
                showToast('No valid data found');
            }
        }

        // ==================== NOTES SYSTEM ====================
        function addNote() {
            const shopName = document.getElementById('noteShopName').value.trim();
            const phone = document.getElementById('notePhone').value.trim();
            const type = document.querySelector('input[name="noteType"]:checked').value;
            
            if (!shopName || !phone) {
                showToast('Please enter shop name and phone');
                return;
            }
            
            // Check for duplicate onboard in notes
            if (type === 'onboarding') {
                const duplicateCheck = checkDuplicateOnboard(phone);
                if (duplicateCheck.isDuplicate) {
                    document.getElementById('duplicateMessage').innerHTML = 
                        `${shopName} (${phone}) already onboarded on ${duplicateCheck.existingEntry.date}`;
                    document.getElementById('noteDuplicateWarning').style.display = 'flex';
                    return;
                }
            }
            
            document.getElementById('noteDuplicateWarning').style.display = 'none';
            
            const now = new Date();
            const timeStr = now.toLocaleTimeString('en-GB', { hour: '2-digit', minute: '2-digit' });
            
            notes.push({
                shopName,
                phone,
                type,
                time: timeStr,
                timestamp: now.getTime()
            });
            
            localStorage.setItem('seviNotes', JSON.stringify(notes));
            
            document.getElementById('noteShopName').value = '';
            document.getElementById('notePhone').value = '';
            
            updateNotesDisplay();
            showToast('Note added');
        }

        function updateNotesDisplay() {
            document.getElementById('noteCount').textContent = notes.length;
            
            if (notes.length === 0) {
                document.getElementById('rawNotes').textContent = 'No notes yet';
                return;
            }
            
            const today = new Date().toLocaleDateString('en-GB', { day: 'numeric', month: 'short' });
            let rawText = today + '\n\n';
            
            const onboards = notes.filter(n => n.type === 'onboarding');
            const trades = notes.filter(n => n.type === 'newtrade');
            
            if (onboards.length > 0) {
                rawText += 'Onboarding\n';
                onboards.forEach((n, i) => {
                    rawText += `${i+1}. ${n.shopName} ${n.phone} (${n.time})\n`;
                });
                rawText += '\n';
            }
            
            if (trades.length > 0) {
                rawText += 'New trade\n';
                trades.forEach((n, i) => {
                    rawText += `${i+1}. ${n.shopName} ${n.phone} (${n.time})\n`;
                });
            }
            
            document.getElementById('rawNotes').textContent = rawText;
        }

        function copyRawNotes() {
            const text = document.getElementById('rawNotes').textContent;
            navigator.clipboard.writeText(text).then(() => {
                showToast('Notes copied');
            });
        }

        function processNotes() {
            if (notes.length === 0) {
                showToast('No notes to process');
                return;
            }
            
            const today = new Date().toISOString().split('T')[0];
            let processed = [];
            let duplicates = [];
            
            notes.forEach(note => {
                // Check for duplicate onboard
                if (note.type === 'onboarding') {
                    const duplicateCheck = checkDuplicateOnboard(note.phone);
                    if (duplicateCheck.isDuplicate) {
                        duplicates.push(note);
                        return; // Skip this note
                    }
                }
                
                // Check if this new trade corresponds to a previous onboard
                if (note.type === 'newtrade') {
                    const existingOnboard = reports.find(r => 
                        r.phoneNumber === note.phone && 
                        r.type === 'onboarding'
                    );
                    
                    if (existingOnboard) {
                        // Add checkmark to the previous onboarding entry
                        existingOnboard.checkmark = true;
                    }
                }
                
                processed.push({
                    id: Date.now() + Math.random(),
                    date: today,
                    shopName: note.shopName,
                    phoneNumber: note.phone,
                    type: note.type,
                    status: note.type === 'newtrade' ? 'traded' : 'not-traded',
                    checkmark: false
                });
            });
            
            if (duplicates.length > 0) {
                let message = 'Duplicate onboardings skipped:\n';
                duplicates.forEach(d => {
                    message += `\n• ${d.shopName} (${d.phone})`;
                });
                document.getElementById('duplicateMessage').textContent = message;
                document.getElementById('noteDuplicateWarning').style.display = 'flex';
            }
            
            if (processed.length > 0) {
                reports = [...reports, ...processed];
                syncToSupabase(processed);
                localStorage.setItem('seviReports', JSON.stringify(reports));
                
                // Clear processed notes
                notes = notes.filter(n => !processed.some(p => p.phoneNumber === n.phone));
                localStorage.setItem('seviNotes', JSON.stringify(notes));
                
                renderHome();
                renderCalendar();
                updateReports();
                updateProgress();
                updateAnalytics();
                destroyCharts();
                initCharts();
                updateNotesDisplay();
                
                showToast(`Processed ${processed.length} notes`);
            }
        }

        function clearNotes() {
            if (confirm('Clear all notes?')) {
                notes = [];
                localStorage.setItem('seviNotes', JSON.stringify(notes));
                updateNotesDisplay();
                document.getElementById('noteDuplicateWarning').style.display = 'none';
                showToast('Notes cleared');
            }
        }

        // ==================== WEEKLY REPORT ====================
        function getWeeklySummary() {
            const weeks = [
                { name: 'Week 1 (1-7 Feb)', trades: 0, onboards: 0, total: 0 },
                { name: 'Week 2 (8-14 Feb)', trades: 0, onboards: 0, total: 0 },
                { name: 'Week 3 (15-21 Feb)', trades: 0, onboards: 0, total: 0 },
                { name: 'Week 4 (22-28 Feb)', trades: 0, onboards: 0, total: 0 }
            ];
            
            reports.forEach(r => {
                const d = new Date(r.date);
                if (d.getMonth() === 1) { // February
                    const day = d.getDate();
                    let weekIndex = 0;
                    if (day >= 1 && day <= 7) weekIndex = 0;
                    else if (day >= 8 && day <= 14) weekIndex = 1;
                    else if (day >= 15 && day <= 21) weekIndex = 2;
                    else if (day >= 22 && day <= 28) weekIndex = 3;
                    
                    if (r.status === 'traded') weeks[weekIndex].trades++;
                    if (r.type === 'onboarding') weeks[weekIndex].onboards++;
                    weeks[weekIndex].total++;
                }
            });
            
            return weeks.map(w => ({
                ...w,
                conversion: w.total ? Math.round((w.trades / w.total) * 100) : 0
            }));
        }

        // ==================== UI RENDERING ====================
        function renderHome() {
            const today = new Date().toISOString().split('T')[0];
            const todayReports = reports.filter(r => r.date === today);
            const yesterday = new Date(Date.now() - 86400000).toISOString().split('T')[0];
            const yesterdayReports = reports.filter(r => r.date === yesterday);
            
            // Today's stats
            const todayTrades = todayReports.filter(r => r.status === 'traded').length;
            const todayOnboards = todayReports.filter(r => r.type === 'onboarding').length;
            
            document.getElementById('todayTrades').textContent = todayTrades;
            document.getElementById('todayOnboards').textContent = todayOnboards;
            
            const conversion = todayReports.length ? Math.round((todayTrades / todayReports.length) * 100) : 0;
            document.getElementById('todayConversion').textContent = conversion + '%';
            
            // Calculate variances with yesterday
            const yesterdayTrades = yesterdayReports.filter(r => r.status === 'traded').length;
            const tradeTrend = yesterdayTrades ? ((todayTrades - yesterdayTrades) / yesterdayTrades * 100).toFixed(0) : 0;
            document.getElementById('tradeTrend').textContent = (tradeTrend > 0 ? '+' : '') + tradeTrend + '%';
            document.getElementById('tradeTrend').className = `stat-trend ${tradeTrend > 0 ? 'trend-up' : tradeTrend < 0 ? 'trend-down' : 'trend-neutral'}`;
            
            const yesterdayOnboards = yesterdayReports.filter(r => r.type === 'onboarding').length;
            const onboardTrend = yesterdayOnboards ? ((todayOnboards - yesterdayOnboards) / yesterdayOnboards * 100).toFixed(0) : 0;
            document.getElementById('onboardTrend').textContent = (onboardTrend > 0 ? '+' : '') + onboardTrend + '%';
            document.getElementById('onboardTrend').className = `stat-trend ${onboardTrend > 0 ? 'trend-up' : onboardTrend < 0 ? 'trend-down' : 'trend-neutral'}`;
            
            // Update poll data
            const totalTrades = reports.filter(r => r.status === 'traded').length;
            const totalShops = reports.length;
            const avgTrades = (totalTrades / 26).toFixed(1); // Approximate days
            
            // Find best day
            const dayCounts = {};
            reports.forEach(r => {
                if (r.status === 'traded') {
                    dayCounts[r.date] = (dayCounts[r.date] || 0) + 1;
                }
            });
            
            let bestCount = 0, bestDate = '';
            Object.entries(dayCounts).forEach(([date, count]) => {
                if (count > bestCount) { bestCount = count; bestDate = date; }
            });
            
            document.getElementById('bestDayValue').textContent = bestCount;
            document.getElementById('bestDayName').textContent = bestDate ? 
                new Date(bestDate).toLocaleDateString('en-GB', { day: 'numeric', month: 'short' }) : '-';
            document.getElementById('avgTrades').textContent = avgTrades;
            document.getElementById('totalShopsPoll').textContent = totalShops;
            document.getElementById('totalTradesPoll').textContent = totalTrades + ' trades';
            
            // Calculate strength and weakness days
            const weekDays = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'];
            const dayAverages = {};
            reports.forEach(r => {
                if (r.status === 'traded') {
                    const d = new Date(r.date);
                    const dayName = weekDays[d.getDay() === 0 ? 6 : d.getDay() - 1];
                    dayAverages[dayName] = (dayAverages[dayName] || 0) + 1;
                }
            });
            
            let strengthDay = { day: '', count: 0 };
            let weaknessDay = { day: '', count: Infinity };
            
            Object.entries(dayAverages).forEach(([day, count]) => {
                if (count > strengthDay.count) {
                    strengthDay = { day, count };
                }
                if (count < weaknessDay.count) {
                    weaknessDay = { day, count };
                }
            });
            
            document.getElementById('strengthDay').innerHTML = `<span>${strengthDay.count || 0}</span> trades on ${strengthDay.day || 'N/A'}`;
            document.getElementById('weaknessDay').innerHTML = `<span>${weaknessDay.count !== Infinity ? weaknessDay.count : 0}</span> trades on ${weaknessDay.day || 'N/A'}`;
            
            // Today's activity table
            let html = '';
            todayReports.forEach(r => {
                const typeText = r.type === 'onboarding' ? 'Onboarded' : 'Verified';
                const typeClass = r.type === 'onboarding' ? 'type-onboard' : 'type-trade';
                const statusText = r.status === 'traded' ? 'Traded' : 'Pending';
                const statusClass = r.status === 'traded' ? 'status-traded' : 'status-pending';
                
                html += `<tr>
                    <td><div class="shop-info">${r.shopName}</div></td>
                    <td><div class="phone-info">${r.phoneNumber}</div></td>
                    <td><span class="type-badge ${typeClass}">${typeText}</span></td>
                    <td><button class="status-badge ${statusClass} clickable" onclick="toggleStatus(${r.id})">${statusText}</button></td>
                </tr>`;
            });
            
            document.getElementById('todayActivityBody').innerHTML = html || 
                '<tr><td colspan="4" class="empty-state"><i class="fas fa-store"></i><br>No activity today. Add data using the + button.</td></tr>';
            
            updateGoals();
        }

        function updateGoals() {
            const goals = MONTHLY_GOALS[1];
            
            // Count only traded status for trades
            const monthTrades = reports.filter(r => r.status === 'traded').length;
            const monthOnboards = reports.filter(r => r.type === 'onboarding').length;
            
            document.getElementById('tradesProgress').textContent = `${monthTrades}/${goals.trades}`;
            document.getElementById('onboardsProgress').textContent = `${monthOnboards}/${goals.onboards}`;
            
            const totalProgress = ((monthTrades + monthOnboards) / (goals.trades + goals.onboards)) * 100;
            const percent = Math.min(100, Math.round(totalProgress));
            document.getElementById('progressPercent').textContent = percent + '%';
            
            const degrees = (percent / 100) * 360;
            document.getElementById('progressRing').style.background = 
                `conic-gradient(white 0deg ${degrees}deg, rgba(255,255,255,0.2) ${degrees}deg 360deg)`;
            
            let rating = 'Poor';
            if (percent >= 90) rating = 'Excellent';
            else if (percent >= 70) rating = 'Good';
            else if (percent >= 50) rating = 'Average';
            
            document.getElementById('monthlyRating').textContent = rating;
        }

        function renderCalendar() {
            const firstDay = new Date(currentYear, currentMonth, 1);
            const lastDay = new Date(currentYear, currentMonth + 1, 0);
            const startOffset = firstDay.getDay() === 0 ? 6 : firstDay.getDay() - 1;
            const today = new Date().toISOString().split('T')[0];
            const trainingEnd = new Date(TRAINING_END_DATE);
            
            // Calculate strength/weakness days
            const weekDays = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'];
            const dayAverages = {};
            reports.forEach(r => {
                if (r.status === 'traded') {
                    const d = new Date(r.date);
                    const dayName = weekDays[d.getDay() === 0 ? 6 : d.getDay() - 1];
                    dayAverages[dayName] = (dayAverages[dayName] || 0) + 1;
                }
            });
            
            let strengthDay = '';
            let weaknessDay = '';
            let maxAvg = 0, minAvg = Infinity;
            Object.entries(dayAverages).forEach(([day, count]) => {
                if (count > maxAvg) { maxAvg = count; strengthDay = day; }
                if (count < minAvg) { minAvg = count; weaknessDay = day; }
            });
            
            let html = '';
            
            // Previous month padding
            for (let i = 0; i < startOffset; i++) {
                html += '<div class="calendar-day disabled"></div>';
            }
            
            // Current month days
            for (let d = 1; d <= lastDay.getDate(); d++) {
                const dateStr = `${currentYear}-${String(currentMonth+1).padStart(2,'0')}-${String(d).padStart(2,'0')}`;
                const dateObj = new Date(dateStr);
                const dayName = weekDays[dateObj.getDay() === 0 ? 6 : dateObj.getDay() - 1];
                const dayReports = reports.filter(r => r.date === dateStr);
                const hasData = dayReports.length > 0;
                const hasCheckmark = dayReports.some(r => r.checkmark === true);
                const dayTrades = dayReports.filter(r => r.status === 'traded').length;
                
                let classes = 'calendar-day';
                
                // Training period (before 14 Feb)
                if (dateObj < trainingEnd) {
                    classes += ' training-period';
                }
                // Future dates
                else if (dateStr > today) {
                    classes += ' future-date';
                }
                // Today
                else if (dateStr === today) {
                    classes += hasCheckmark ? ' has-checkmark' : hasData ? ' has-data' : '';
                }
                // Past dates with data
                else if (hasData) {
                    if (dayName === strengthDay && dayTrades > 0) {
                        classes += ' strength-day';
                    } else if (dayName === weaknessDay && dayTrades > 0) {
                        classes += ' weakness-day';
                    } else if (hasCheckmark) {
                        classes += ' has-checkmark';
                    } else {
                        classes += ' has-data';
                    }
                }
                // Past dates without data
                else {
                    classes += ' past-view';
                }
                
                // Sundays (15th, 22nd were Sundays)
                const isSunday = dateObj.getDay() === 0;
                if (isSunday) {
                    classes += ' disabled';
                }
                
                html += `<div class="${classes}" onclick="selectDate('${dateStr}')">
                    <span class="day-number">${d}</span>
                    ${hasData ? '<span class="day-indicator"></span>' : ''}
                </div>`;
            }
            
            document.getElementById('calendarDays').innerHTML = html;
            document.getElementById('calendarMonth').textContent = 'February 2026';
        }

