            navigator.clipboard.writeText(report).then(() => showToast('Report copied'));
        }

        function shareToWhatsApp() {
            const report = document.getElementById('dailyReport').textContent;
            window.open(`https://wa.me/?text=${encodeURIComponent(report)}`, '_blank');
        }

        function updateProgress() {
            // Calculate streak
            let streak = 0;
            let checkDate = new Date();
            while (true) {
                const dateStr = checkDate.toISOString().split('T')[0];
                const dayHasTraded = reports.some(r => r.date === dateStr && r.status === 'traded');
                const dayHasCheckmark = reports.some(r => r.date === dateStr && r.checkmark === true);
                if (dayHasTraded || dayHasCheckmark) {
                    streak++;
                    checkDate.setDate(checkDate.getDate() - 1);
                } else break;
            }
            document.getElementById('streakCount').textContent = streak;
            
            // Best day (by traded status)
            const dayCounts = {};
            reports.forEach(r => {
                if (r.status === 'traded') {
                    dayCounts[r.date] = (dayCounts[r.date] || 0) + 1;
                }
            });
            
            let bestCount = 0, bestDate = '';
            Object.entries(dayCounts).forEach(([date, count]) => {
                if (count > bestCount) { bestCount = count; bestDate = date; }
            });
            
            document.getElementById('bestDayCount').textContent = bestCount;
            document.getElementById('bestDayDate').textContent = bestDate ? 
                new Date(bestDate).toLocaleDateString('en-GB', { day: 'numeric', month: 'short' }) : '-';
            document.getElementById('totalShops').textContent = reports.length;
        }

        function updateAnalytics() {
            const totalTrades = reports.filter(r => r.status === 'traded').length;
            const totalOnboards = reports.filter(r => r.type === 'onboarding').length;
            const totalCheckmarked = reports.filter(r => r.checkmark === true).length;
            
            document.getElementById('totalTrades').textContent = totalTrades;
            document.getElementById('totalOnboards').textContent = totalOnboards;
            document.getElementById('totalCheckmarked').textContent = totalCheckmarked;
            
            const conversion = reports.length ? Math.round((totalTrades / reports.length) * 100) : 0;
            document.getElementById('totalConversion').textContent = conversion + '%';
        }

