        function toggleStatus(id) {
            const report = reports.find(r => r.id === id);
            if (report) {
                // Check if this is today's date
                const today = new Date().toISOString().split('T')[0];
                if (report.date !== today) {
                    showToast('Can only update status for today');
                    return;
                }
                
                report.status = report.status === 'traded' ? 'not-traded' : 'traded';
                
                localStorage.setItem('seviReports', JSON.stringify(reports));
                syncToSupabase([report]);
                
                renderHome();
                if (selectedDate) selectDate(selectedDate);
                updateReports();
                updateProgress();
                updateAnalytics();
                destroyCharts();
                initCharts();
                
                showToast('Status updated');
            }
        }

        function changeMonth(delta) {
            currentMonth += delta;
            if (currentMonth < 0) { currentMonth = 11; currentYear--; }
            else if (currentMonth > 11) { currentMonth = 0; currentYear++; }
            renderCalendar();
        }

        function updateReports() {
            const today = new Date().toISOString().split('T')[0];
            const todayReports = reports.filter(r => r.date === today);
            
            if (todayReports.length === 0) {
                document.getElementById('dailyReport').textContent = 'No data for today. Add data using the + button.';
